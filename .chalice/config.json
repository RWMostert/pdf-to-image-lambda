{
  "version": "2.0",
  "app_name": "pdf2image",
  "stages": {
    "dev": {
      "api_gateway_stage": "api",
      "lambda_memory_size": 2048,
      "lambda_timeout": 900
    }
  },
  "environment_variables": {
    "ORIGIN_BUCKET": "pdf-bucket-0",
    "DESTINATION_BUCKET": "converted-image-bucket-1",
    "FMT": "png",
    "DPI": "300"
  }
}
